{
  "name": "@theanurin/http-dump",
  "version": "0.3.1",
  "description": "Dumps HTTP(s) requests into files",
  "main": "src/index.js",
  "scripts": {
    "clean": "find src \\( -name '*.js' -o -name '*.d.ts' -o -name '*.map' \\) -exec rm {} \\; ;",
    "build": "tsc",
    "start": "npm run build && ./bin/app.js --config-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theanurin/http-dump.git"
  },
  "keywords": [
    "HTTP",
    "Dump"
  ],
  "author": "Maksym Anurin",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/theanurin/http-dump/issues"
  },
  "homepage": "https://github.com/theanurin/http-dump#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@freemework/common": "^0.10.15",
    "@freemework/hosting": "^0.10.15"
  }
}
